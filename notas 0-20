nota = int(input("Qual é a tua nota de avaliação: "))

if nota==0 and nota<5:
    print("Péssimo")
if nota>=5 and nota<8:
    print("Mau")
if nota>=8 and nota<10:
    print("Insuficiente")
if nota>=10 and nota<12:
    print("Suficiente")
if nota>=12 and nota<16:
    print("Bom")
if nota>=16 and nota<=20:
    print("Excelente")
